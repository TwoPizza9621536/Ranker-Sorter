{
  "name": "ranker-sorter",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "browserslist": [
    "> 0.3%",
    "defaults",
    "last 2 versions",
    "not dead",
    "not samsung <= 23"
  ],
  "hintConfig": {
    "extends": [
      "development"
    ],
    "hints": {
      "typescript-config/is-valid": "off"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0 && stylelint src/*.css --report-needless-disables --max-warnings 0 && prettier src -c",
    "format": "eslint --fix src && stylelint --fix src/*.css && prettier src -w",
    "preview": "vite preview"
  },
  "dependencies": {
    "js-elo-system": "^1.2.2",
    "json-schema-library": "^9.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "victory": "^36.6.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.55.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-use-nesting": "^4.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
